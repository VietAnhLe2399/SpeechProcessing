   "source": [
    "def search_word(word_to_search, sample_max):\n",
    "    for word in word_to_search:\n",
    "        sample_count = 0\n",
    "        out_df = pd.DataFrame(columns=['Họ và tên', 'Chủ đề', 'Tên file', 'Nội dung'])\n",
    "        for i in range(len(df)):\n",
    "            for cate in categories:\n",
    "                if word in str(df.loc[i, cate]):\n",
    "                    text = df.loc[i, cate].split('\\n')[1:]\n",
    "                    for j in range(1, len(text), 2):\n",
    "                        if re.search(word, text[j], flags=re.I):\n",
    "#                             print(sample_count, '\\t', df.loc[i, 'Họ và tên'], '----', cate, '----', text[j-1])\n",
    "#                             print(text[j])\n",
    "                            out_df.loc[sample_count, 'Họ và tên'] = df.loc[i, 'Họ và tên']\n",
    "                            out_df.loc[sample_count, 'Chủ đề'] = cate\n",
    "                            out_df.loc[sample_count, 'Tên file'] = text[j-1]\n",
    "                            out_df.loc[sample_count, 'Nội dung'] = text[j]\n",
    "                            sample_count+=1\n",
    "#                             if sample_count >= sample_max:\n",
    "        out_df.to_excel(\"%s.xlsx\" %word, index = False)\n",
    "        print('Saved file as %s.xlsx' %word)\n",
    "#                                 return"
   ]
  },
  {
