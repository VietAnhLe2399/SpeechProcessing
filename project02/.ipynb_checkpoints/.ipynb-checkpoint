   "source": [
    "def preprocess_text(line):\n",
    "    text = line.rsplit(\",\", 1)[0].strip(\" \\n\\r\\t\")\n",
    "    text = re.sub(\"(?<=[a-z])[-]?(?=[0-9A-Z])\", \" \", text)\n",
    "    text = re.sub(\"(?<=[0-9])(?=[A-Z][a-z]+)\", \" \", text)\n",
    "    return text.lower()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "index = 0\n",
    "\n",
    "for i in range(len(df)):\n",
    "    for cate in categories:\n",
    "        cell = df.loc[1, cate].split('\\n')[1:]\n",
    "        text = \"\"\n",
    "        for i in range(len(cell)):\n",
    "            if i % 2 == 1:\n",
    "                text += preprocess_text(cell[i])\n",
    "        df_content.loc[index] = text\n",
    "        index+=1"
   ]
  },
