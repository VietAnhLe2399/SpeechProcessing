   "source": [
    "# Count number of occurrences of 1 word in all doc\n",
    "def count_word(word):\n",
    "    count = 0\n",
    "    for i in range(len(df_content)):\n",
    "#         for cate in categories:\n",
    "        if word in str(df_content.loc[i, 'content']):\n",
    "            count += sum(1 for _ in re.finditer(re.escape(word), df_content.loc[i, 'content']))\n",
    "#                 print(count, '\\t', df.loc[i, 'Họ và tên'], '\\t'*3, cate)\n",
    "    return count"
   ]
  },
